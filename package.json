{
  "name": "ramda-lens-groups",
  "version": "1.2.3",
  "description": "An easy way to manage a set of ramda lenses focused on props of a particular object type",
  "author": "Steven Saunders",
  "license": "ISC",
  "main": "lib/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib dist es",
    "build:commonjs": "cross-env NODE_ENV=production babel src --out-dir lib",
    "build:es": "cross-env NODE_ENV=production babel src --out-dir es",
    "build:umd": "cross-env NODE_ENV=development webpack src/index.js dist/ramda-lens-groups.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack src/index.js dist/ramda-lens-groups.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "prepublish": "npm run clean && npm test && npm run build",
    "test": "mocha --require babel-core/register",
    "testDocs": "babel-node docs/testDocExamples.js; babel-node docs/testApiExamples.js",
    "testWhyLenses": "babel-node docs/testWhyLenses.js",
    "testIndex": "babel-node ./index.js",
    "watch:test": "onchange 'src/**/*.js' 'test/**/*.js' -- yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SteveAtSentosa/ramda-lens-groups.git"
  },
  "keywords": [
    "ramda",
    "lens"
  ],
  "bugs": {
    "url": "https://github.com/SteveAtSentosa/ramda-lens-groups/issues"
  },
  "homepage": "https://github.com/SteveAtSentosa/ramda-lens-groups#readme",
  "dependencies": {
    "ramda": "^0.27.0",
    "ramda-adjunct": "^2.26.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-mocha": "^6.3.0",
    "mocha": "^7.1.1",
    "onchange": "^6.1.0",
    "rimraf": "^3.0.2",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
